{
  "manifest_version": 2,
  "name": "Remove headers",
  "description": "This extension removes header from the current page",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ] ,
      "js": [
        "lib/jquery-3.1.1.js",
        "lib/modernizr-3.5.0.js",
        "lib/underscore-1.8.3.js",
        "lib/css-selector-generator-1.0.4.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
